// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Market {
  id   Int    @id @default(autoincrement())
  name String
}

model Product {
  id    Int    @id @default(autoincrement())
  name  String
  brand String
  size  Float
  unit  String
}

model Price {
  id        Int      @id @default(autoincrement())
  price     Float
  date      DateTime
  marketId  Int
  productId Int
}
